{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "dist",
    "rewrites": [

			{
				"source": "/api/webhooks/github",
				"function": "github_webhook"
      },
      {
				"source": "/api/webhooks/:provider",
				"function": "ci_webhook"
			},
			{
        "source": "**",
        "destination": "/index.html"
			}
    ]
  }
}
